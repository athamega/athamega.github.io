					var autoTimer;
					
					$(document).ready(function() {
						$('#global-search').autocomplete({
							source: autocompleteQuery,
							delay: 500,
							appendTo: 'div.global-search-wrapper'
						});
					});
					
					function autocompleteQuery(request, response)
			    {
						var query = $('#global-search').val();
						$.ajax({
							method: "POST",
							url: "/search/",
							data: { action: 'relatedSearch', query: query },
							dataType: "json"
						}).done(function(output) {
							var vals = [];
							for(var i = 0; i < output.length; i++)
							{
								vals[i] = {label: output[i], value: output[i]};
							}
							response(vals);
						});
			    }
				</script>
				
